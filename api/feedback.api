syntax = "v1"

type (
	FeedbackReq {
		StudentID string `json:"studentid"`
		Content string `json:"content"`
	}
	FeedbackRes {
		Base Base  
		Data []FeedbackReq `json:"feedback"`
	}
)

@server (
	prefix: "/api/v1"
	jwt:    Auth
)

service seat-api {
	@handler submitFeedback
	post /feedback (FeedbackReq) returns (GeneralRes)
	@handler getFeedback
	get /feedback (FeedbackReq) returns (FeedbackRes)
}

