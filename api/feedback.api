syntax = "v1"

type (
  FeedbackReq {
    Content string `json:"content"`
  }

  EmptyData {}

  GeneralRes {
    Code    int       `json:"code"`
    Message string    `json:"message"`
    Data    EmptyData `json:"data"`
  }
)

@server(
    jwt: Auth
)

service seat-api {
  @handler submitFeedback
  post /feedback (FeedbackReq) returns (GeneralRes)
}
